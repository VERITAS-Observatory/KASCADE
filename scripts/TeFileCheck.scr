let iFilesNotFound=0;

let j=$iEntryStart
echo 'TeFileCheck: Searching  for Te files in '$1
rm $1'.Result'

while test $j -le $iEntryEnd
  do      
   let iShStart=${start[$j]}
   let iShEnd=${end[$j]}
   e=${energy[$j]}

   let k=$iShStart
   while test $k -le $iShEnd
     do
      #Make up the file name
      TeFile=$TeName'GeV'$e'te.d'$k
      Found=0
      {
       while read FileName
         do
	   if [ "$TeFile" = "$FileName" ]; then
             Found=1
             break
           fi
         done
      }<$1
      if [ "$Found" = "0" ]; then
        echo 'File Not found: '$TeFile >>$1'.Result'
      fi
      let k=k+1
     done
   let j=j+1
  done
