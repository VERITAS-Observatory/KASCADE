
let step=1
while [ $step -le "12" ] ; do
        if [ $step -le "4" ]; then
                tail -n200 ~/kasaom_gaea$step'_g.log'
                echo gaea$step
	elif [ $step -le "8" ]; then
        	let proc=step-4
		tail -n200 ~/kasaom_knyr$proc'_g.log'
		echo knyr$proc
        else
		let proc=step-8
		tail -n200 ~/kasaom_saji$proc'_g.log'
		echo saji$proc
	fi
	let step=step+1
	read a
done
