#bin/bash
#$1 start entry
#$2 end entry
#$3 = "[]" or not.
let start=$1
let end=$2

if [ -e "RemoveQueue.tmp" ]; then
   rm RemoveQueue.tmp
fi

qstat -u $USER | grep $USER >RemoveQueue.tmp

{
 while read JobID Rest
   do
    JobID=${JobID%%.*}
    let JOBID=$JobID
    if [ "$JOBID" -ge "$start" ] && [ "$JOBID" -le "$end" ]; then
      qdel $JOBID
    fi
   done
}<RemoveQueue.tmp